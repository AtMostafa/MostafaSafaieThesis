    "latex-workshop.latex.recipe.default": "lastUsed",
    "editor.fontSize": 20,
    "files.exclude": {
        "**/.vscode/": true,
        "**/*.fdb_latexmk": true,
        "**/*.fls": true,
        "**/*.aux": true,
        "**/*.bcf": true,
        "thesis.bib": true,
        "**/*.ist": true,
        "thesis.pdf": true,
        "**/*.run.xml": true,
        "**/*.synctex.gz": true,
        "**/*.bbl": true,
        "**/*.blg": true,
        "**/*.glo": true,
        "**/*.lof": true,
        "**/*.lot": true,
        "**/*.out": true,
        "**/*.toc": true,
        "**/*.gls": true,
        "**/*.glg": true,
        "**/*.log": true
    },
    "latex-workshop.latex.tools": 
    [
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "--shell-escape", // if you want to have the shell-escape flag
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        },
        {
            "name": "biber",
            "command": "biber",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        }
    ],
    "latex-workshop.latex.recipes": 
    [
        {
            "name": "pdflatex -> biblatex -> pdflatex*2",
            "tools": [
                "pdflatex",
                "biber",
                "pdflatex",
                "pdflatex"
            ]
        }
    ],
